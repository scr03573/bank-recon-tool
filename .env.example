# Sage Intacct Credentials
INTACCT_SENDER_ID=your_sender_id
INTACCT_SENDER_PASSWORD=your_sender_password
INTACCT_USER_ID=your_user_id
INTACCT_USER_PASSWORD=your_user_password
INTACCT_COMPANY_ID=your_company_id

# Market Data APIs
# Intrinio (https://intrinio.com - premium financial data)
INTRINIO_API_KEY=your_intrinio_api_key

# FRED API (https://fred.stlouisfed.org/docs/api/api_key.html - free)
FRED_API_KEY=your_fred_api_key

# Market Data Priority
# Options: intrinio_first, yfinance_first, intrinio_only, yfinance_only, best_available
MARKET_DATA_PRIORITY=yfinance_first

# Cache duration for market data (minutes)
MARKET_DATA_CACHE_MINUTES=15

# Enable economic validation in matching
ENABLE_ECONOMIC_VALIDATION=true

# Matching Thresholds
FUZZY_MATCH_THRESHOLD=85
DATE_TOLERANCE_DAYS=5
AMOUNT_TOLERANCE_PERCENT=0.01

# Database
DATABASE_URL=sqlite:///./reconciliation.db